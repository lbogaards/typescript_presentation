{
    "version": "1.5.0-beta",
    "compilerOptions": {
        "target": "es5",
        "jsx": "react",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "declaration": false,
        "noImplicitAny": false,
        "removeComments": true,
        "noLib": false,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true,
        "outDir": "./dist/"
    },
    "filesGlob": [
        "./**/*.ts"
    ],
    "files": [
        "./app.ts",
        "./ball.ts",
        "./brick.ts",
        "./config.ts",
        "./levelOne.ts",
        "./lib/excalibur-0.5.1.d.ts",
        "./paddle.ts",
        "./state.ts"
    ]
}
